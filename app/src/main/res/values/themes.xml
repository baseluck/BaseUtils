<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SchoolDormitoryMobile" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color1094f5</item>
        <item name="colorPrimaryVariant">@color/color1094f5</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/color1094f5</item>
        <item name="colorSecondaryVariant">@color/color1094f5</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/color1094f5</item>
        <!-- Customize your theme here. -->
        <item name="android:windowActionBarOverlay">false</item>
        <!-- Support library compatibility -->
        <item name="windowActionBarOverlay">false</item>
    </style>

    <style name="myLogin" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorControlNormal">#eeeeee</item>
        <!-- AppCompatEditText选择的底线颜色 -->
        <item name="colorControlActivated">#eeeeee</item>
    </style>

    <!--dialog布局-->
    <style name="ShareDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimAmount">0.3</item>
    </style>
</resources>